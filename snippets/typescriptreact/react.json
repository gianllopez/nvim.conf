{
  "@react/rfc": {
    "prefix": "rfc",
    "description": "React functional componente with its type-safe props",
    "body": [
			"type Props = {};",
			"",
			"export function $1({}: Props) {",
			"\treturn <$2>Hello from `$1`</$2>;",
			"}"
    ]
  },
	"@react/rut": {
    "prefix": "rut",
    "description": "React `useState()` hook implementation",
    "body": [
			"const [$1, $2] = useState<$3>($4);"
    ]
  }
}
