{
	"@python/pdb": {
		"prefix": "pdb",
		"description": "Create a Python debug point",
		"body": [
			"__import__(\"pdb\").set_trace()"
		]
	},
	"@python/def": {
		"prefix": "def",
		"description": "Create a Python function/method with its type hints",
		"body": [
			"def $1($2)->$3:",
			"\t$4"
		]
	}
}
